{
    "name": "jubaopen/jwt-authentication-bundle",
    "description": "This bundle provides JWT authentication for your Symfony REST API",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "zhanghua",
            "email": "2533058383@qq.com"
        },
        {
            "name": "zhaozhuobin",
            "email": "zb_zhao@msn.cn"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/security-bundle": "^2.8|^3.0",
        "symfony/console": "^2.8|^3.0",
        "namshi/jose": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "symfony/phpunit-bridge": "~3.2",
        "symfony/browser-kit": "^2.8|^3.0",
        "symfony/dom-crawler": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "friendsofphp/php-cs-fixer": "^1.1",
        "lcobucci/jwt": "~3.2"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "JWTAuthenticationBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/simple-phpunit",
            "ENCODER=lcobucci vendor/bin/simple-phpunit",
            "PROVIDER=jbp_jwt vendor/bin/simple-phpunit"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
